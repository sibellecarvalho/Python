def jogar():
    print("*********************************")
    print("***Bem vindo ao jogo da Forca!***")
    print("*********************************")

    palavra_secreta = "Melancia"

    enforcou= False
    acertou= False

    while(not enforcou and not acertou):
        print("jogando...")

    print("Fim de jogo")

if(__name__=="__main"):
    jogar()
